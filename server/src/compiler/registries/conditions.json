{
	"$schema": "./conditions.schema.json",
	"predicates":{
		"damage_type":{
			"bypasses_armor":{
				"type":"bool"
			},
			"bypasses_invulnerability":{
				"type":"bool"
			},
			"bypasses_magic":{
				"type":"bool"
			},
			"explosion":{
				"path":["is_explosion"],
				"type":"bool"
			},
			"fire":{
				"path":["is_fire"],
				"type":"bool"
			},
			"magic":{
				"path":["is_magic"],
				"type":"bool"
			},
			"projectile":{
				"path":["is_projectile"],
				"type":"bool"
			},
			"lightning":{
				"path":["is_lightning"],
				"type":"bool"
			},
			"direct_entity":{
				"type":"compound",
				"context": {
					"predicate":"entity"
				}
			},
			"source_entity":{
				"type":"compound",
				"context": {
					"predicate":"entity"
				}
			}
		},
		"entity":{
			"distance":{
				"type":"compound",
				"context": {
					"predicate":"distance"
				}
			},
			"effects":{
				"type":"compound",
				"context": {
					"keys": "effects",
					"values": {
						"type":"compound",
						"context": {
							"predicate":"single_effect"
						}
					}
				}
			},
			"equipment":{
				"type":"compound",
				"context": {
					"keys": "baseEquipmentSlots",
					"values": {
						"type":"compound",
						"context": {
							"predicate":"item"
						}
					}
				}
			},
			"on_fire":{
				"path":["flags","is_on_fire"],
				"type":"bool"
			},
			"sneaking":{
				"path":["flags","is_sneaking"],
				"type":"bool"
			},
			"sprinting":{
				"path":["flags","is_sprinting"],
				"type":"bool"
			},
			"swimming":{
				"path":["flags","is_swimming"],
				"type":"bool"
			},
			"baby":{
				"path":["flags","is_baby"],
				"type":"bool"
			},
			"location":{
				"type":"compound",
				"context": {
					"predicate":"location"
				}
			},
			"player":{
				"type":"compound",
				"context": {
					"predicate":{
						"advancements":{
							"type":"compound",
							"context": {
								"values": {
									"type":"advancement"
								}
							}
						},
						"gamemode":{
							"type":"enum",
							"context": {
								"registry": "gamemodes"
							}
						},
						"level":{
							"type":"int_range"
						},
						"recipes":{
							"type":"compound",
							"context": {
								"values": {
									"type":"bool"
								}
							}
						},
						"stats":{
							"type":"list",
							"context": {
								"item":"compound",
								"context": {
									"predicate":{
										"type":{
											"type":"enum",
											"context": {
												"values": [
													"minecraft:custom",
													"minecraft:crafted",
													"minecraft:used",
													"minecraft:broken",
													"minecraft:mined",
													"minecraft:killed",
													"minecraft:picked_up",
													"minecraft:dropped",
													"minecraft:killed_by"
												]
											}
										},
										"stat":{
											"type":"string"
										},
										"value":{
											"type":"int_range"
										}
									}
								}
							}
						}
					}
				}
			},
			"nbt":{
				"type":"nbt",
				"context": {
					"entry":{
						"from": ["type"]
					},
					"registry": "entities"
				},
				"post_processor":"stringify"
			},
			"team":{
				"type":"variable",
				"context": {
					"type":"team"
				}
			},
			"type":{
				"type":"enum",
				"context": {
					"registry": "entities"
				}
			},
			"targeted_entity":{
				"type":"compound",
				"context": {
					"predicate":"entity"
				}
			},
			"vehicle":{
				"type":"compound",
				"context": {
					"predicate":"entity"
				}
			}
		},
		"single_effect": {
			"amplifier":{
				"type":"int_range"
			},
			"duration":{
				"type":"int_range"
			}
		},
		"distance":{
			"absolute":{
				"type":"double_range"
			},
			"horizontal":{
				"type":"double_range"
			},
			"x":{
				"type":"double_range"
			},
			"y":{
				"type":"double_range"
			},
			"z":{
				"type":"double_range"
			}
		},
		"enchantments":{
			"#values": {
				"type":"compound",
				"context": {
					"predicate":{
						"id":{
							"type":"enum",
							"context": {
								"registry": "enchantments"
							},
							"path": ["enchantment"]
						},
						"levels":{
							"type":"int_range"
						}
					}
				}
			}
		},
		"location":{
			"biome":{
				"type":"enum",
				"context": {
					"registry": "biomes"
				}
			},
			"block":{
				"type":"compound",
				"context": {
					"predicate":{
						"block":{
							"type":"enum",
							"context": {
								"registry": "blocks"
							}
						},
						"tag":{
							"type":"string"
						},
						"nbt":{
							"type":"nbt",
							"context": {
								"registry": "tileEntities",
								"entry":{
									"from": ["block"]
								}
							},
							"post_processor":"stringify"
						},
						"state":{
							"type":"blockstate"
						}
					}
				}
			},
			"dimension":{
				"type":"enum",
				"context": {
					"registry": "dimensions"
				}
			},
			"feature":{
				"type":"enum",
				"context": {
					"registry": "features"
				}
			},
			"fluid":{
				"type":"compound",
				"context": {
					"predicate":{
						"fluid":{
							"type":"enum",
							"context": {
								"registry": "fluids"
							}
						},
						"tag":{
							"type":"string"
						},
						"nbt":{
							"type":"nbt",
							"context": {
								"registry": "tileEntities",
								"entry":{
									"from": ["block"]
								}
							},
							"post_processor":"stringify"
						},
						"state":{
							"type":"blockstate"
						}
					}
				}
			},
			"light":{
				"path": ["light","light"],
				"type":"int_range"
			},
			"x":{
				"type":"double_range"
			},
			"y":{
				"type":"double_range"
			},
			"z":{
				"type":"double_range"
			},
			"smokey":{
				"type":"bool"
			}
		},
		"item":{
			"count":{
				"type":"int_range"
			},
			"durability":{
				"type":"int_range"
			},
			"enchantments":{
				"type":"compound",
				"context": {
					"predicate":"enchantments"
				}
			},
			"stored_enchantments":{
				"type":"compound",
				"context": {
					"predicate":"enchantments"
				}
			},
			"id":{
				"type":"enum",
				"context": {
					"registry": "items"
				}
			},
			"nbt":{
				"type":"nbt",
				"context": {
					"registry": "items",
					"entry":{
						"from": ["id"]
					}
				},
				"post_processor":"stringify"
			}
		}
	},
	"loot_conditions": {
		"block_state":{
			"realKey": "block_state_property",
			"params": {
				"block":{
					"type":"block_id"
				},
				"properties":{
					"type":"blockstate"
				}
			}
		},
		"damage_source":{
			"realKey": "damage_source_properties",
			"params": {
				"predicate":{
					"type":"compound",
					"context": {
						"predicate":"damage_type"
					}
				}
			}
		},
		"entity_info":{
			"realKey": "entity_properties",
			"params": {
				"entity":{
					"type":"enum",
					"context": {
						"values": ["this","killer","killer_player"]
					}
				},
				"predicate":{
					"type":"compound",
					"context": {
						"predicate":"entity"
					}
				}
			}
		},
		"entity_scores":{
			"params": {
				"entity":{
					"type":"enum",
					"context": {
						"values": ["this","killer","killer_player"]
					}
				},
				"scores":{
					"type":"compound",
					"context": {
						"values": {
							"type":"int_range"
						}
					}
				}
			}
		},
		"killed_by_player":{
			"params": {
				"inverse":{
					"type":"bool"
				}
			}
		},
		"location_check":{
			"params": {
				"predicate":{
					"type":"compound",
					"context": {
						"predicate":"location"
					}
				},
				"offsetX":{
					"type":"int",
					"writeonly": true
				},
				"offsetY":{
					"type":"int",
					"writeonly": true
				},
				"offsetZ":{
					"type":"int",
					"writeonly": true
				}
			}
		},
		"tool_check":{
			"realKey": "match_tool",
			"params": {
				"predicate":{
					"type":"compound",
					"context": {
						"predicate":"item"
					}
				}
			}
		},
		"random":{
			"realKey": "random_chance",
			"params": {
				"chance":{
					"type":"float"
				}
			}
		},
		"random_with_looting":{
			"realKey": "random_chance_with_looting",
			"params": {
				"chance":{
					"type":"float"
				},
				"looting_multiplier":{
					"type":"float"
				}
			}
		},
		"survives_explosion":{
			"params": {}
		},
		"table_bonus":{
			"params": {
				"enchantment":{
					"type":"enum",
					"context": {
						"registry": "enchantments"
					}
				},
				"chances":{
					"type":"list",
					"context": {
						"item":"float"
					}
				}
			}
		},
		"time_check":{
			"params": {
				"value":{
					"type":"int_range"
				},
				"period":{
					"type":"int"
				}
			}
		},
		"weather_check":{
			"params": {
				"raining":{
					"type":"bool",
					"writeonly": true
				},
				"thundering":{
					"type":"bool",
					"writeonly": true
				}
			}
		}
	}
}